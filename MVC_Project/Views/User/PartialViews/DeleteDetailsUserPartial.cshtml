@model UserViewModel


@* Name Input *@
<div>
    <label class="col-form-label" asp-for="Fname"></label>
    <input type="text" asp-for="Fname" class="form-control" readonly />
</div>

@* Email input *@
<div>
    <label class="col-form-label" asp-for="Email"></label>
    <input asp-for="Email" class="form-control" readonly />
</div>

@* Phone Input *@
<div>
    <label class="col-form-label" asp-for="phoneNumber"></label>
    <input type="text" asp-for="phoneNumber" class="form-control" readonly />
</div>

@* Roles Input *@
<div>
    <label class="col-form-label" asp-for="Roles"></label>
    <input type="text" asp-for="Roles" class="form-control" readonly />
</div>

