@model int
<td>
    <a asp-action="Details" asp-route-id="@Model">
        <i class="fas fa-eye text-success"></i>
    </a>
</td>
<td>
    <a asp-action="Edit" asp-route-id="@Model">
        <i class="fas fa-edit text-warning"></i>
    </a>
</td>
<td>
    <a asp-action="Delete" asp-route-id="@Model">
        <i class="fas fa-trash text-danger"></i>
    </a>
</td>
